{
  "compilerOptions": {
    "target": "ES2020",
    "jsx": "react-jsx",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
"noUnusedLocals": true,
"noUnusedParameters": true,
"noImplicitAny": true,

    "typeRoots": ["./types", "./node_modules/@types"]  // <-- Fixed
  },
  "include": ["src", "types"]  // <-- Removed trailing comma
}
